#include "/home/josh/.colors/solarized-light"

Xft.autohint:	0
Xft.lcdfilter:	lcddefault
Xft.antialias:	1
Xft.hinting: 1
Xft.hintstyle:	hintfull
Xft.rgba:	rgb
Xft.dpi:	96

! transparency
!urxvt*depth:32

urxvt*cursorBlink:true
urxvt*scrollBar:false
urxvt*internalBorder: 4

URxvt.letterSpace: -1
!URxvt.letterSpace: 0

!URxvt.font: 9x15
!URxvt.boldFont: 9x15bold
!URxvt*font: xft:terminus:pixelsize=15
!URxvt*boldfont: xft:terminus:bold:pixelsize=15
!URxvt.font: xft:Liberation Mono for Powerline:size=8
!URxvt.font: xft:Anonymous Pro:size=11
!URxvt.font: xft:Deja Vu Sans Mono:size=9
!URxvt.boldFont: xft:Deja Vu Sans Mono:bold:size=9
!URxvt.font: 9x15
!URxvt.boldFont: 9x15bold
!URxvt.font: -misc-fixed-medium-r-normal--15-140-75-75-c-90-iso8859-1
!URxvt.boldFont: -misc-fixed-medium-r-normal--15-140-75-75-c-90-iso8859-1
URxvt.font: xft:Cousine:size=8
URxvt.boldFont: xft:Cousine:bold:size=8
!URxvt.font: xft:Anonymous Pro for Powerline:size=8
!URxvt.boldFont: xft:Anonymous Pro for Powerline:bold:size=8
URxvt.iso14755 : false
URxvt.saveLines: 8192 
URxvt.perl-ext-common: default,matcher,vtwheel,clipboard,font-size
URxvt.keysym.C-S-C: perl:clipboard:copy
URxvt.keysym.C-S-V: perl:clipboard:paste
URxvt.keysym.C-equal: perl:font-size:increase
URxvt.keysym.C-minus: perl:font-size:decrease

! clickable URLs
URxvt.url-launcher:      /usr/bin/chromium
URxvt.matcher.button:   1
